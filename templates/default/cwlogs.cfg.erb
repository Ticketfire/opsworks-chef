[general]

[<%= node[:opsworks][:stack][:name] %>]
datetime_format = [%Y-%m-%d %H:%M:%S]
log_group_name = <%= node[:opsworks][:stack][:name].gsub(' ', '_') + '_rails_' + @rails_env %>
file = 'srv/www/<%= @application_name %>/shared/log/<%= @rails_env %>.log'
log_stream_name = <%= node[:opsworks][:instance][:hostname] %>
